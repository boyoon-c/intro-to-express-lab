<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>F1 Drivers</h1>
    <% if(error){ %>
        <h2><%= error %> </h2>
    <% } %> 
    <table>
        <thead>
            <tr>
                <th>Driver Name</th>
                <th>Team</th>
                <th>2021 Driver Standing</th>
            </tr>
        </thead>
        <tbody>
        <tr>
        <% racers.forEach(function(racer) { %>
            <td>
                <%= racer.name %> 
            </td>
            <td> 
                <%= racer.team %> 
            </td>
            <td> 
                <%= racer.pos %> 
            </td>
        </tr>
        <% }) %> 
        </tobdy>
    </table>
</body>
</html>